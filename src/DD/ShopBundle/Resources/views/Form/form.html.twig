{% block form_errors -%}
    {% if errors|length > 0 -%}
        <ul class="text-error">
            {%- for error in errors -%}
                <li>{{ error.message }}</li>
            {%- endfor -%}
        </ul>
    {%- endif %}
{%- endblock form_errors %}
